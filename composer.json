{
    "name": "iceagency/lumberjack-sentry-service-provider",
    "type": "project",
    "license": "MIT",
    "version": "1.0.11",
    "description": "A Service Provider for the Lumberjack framework that adds a Sentry integration.",
    "homepage": "https://github.com/ICEAgency/lumberjack-sentry-service-provider",
    "authors": [
        {
            "name": "Luke Hopkins",
            "email": "luke@theiceagency.co.uk",
            "homepage": "https://github.com/iceagency-lukehopkins"
        }
    ],
    "require": {
        "php": "^7.0",
        "rareloop/lumberjack-core": "^3.2.0",
        "sentry/sentry": "^1.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.2",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "IceAgency\\Lumberjack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IceAgency\\Lumberjack\\Test\\": "tests"
        }
    }
}
